
#include <stdio.h>

int main()
{
    for (int i=1;i<=5;i++)
    {
        for (int j=1;j<=5-i;j++)
        {
            printf(" ");
        }
        for (int j=6-i;j<=5;j++)
        {
            printf("%d",j);
        }
        for (int j=4;j>=6-i;j--)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    for (int i=1;i<=5;i++)
    {
        for (int j=1;j<=i;j++)
        {
            printf(" ");
        }
        for (int k=i;k<=5;k++)
        {
            printf("%d",k);
        }
        for (int k=5;k>=i;k--)
        {
            printf("%d",k);
        }
        
    
        
        
    printf("\n");    
    }
    

    return 0;
}
